<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.shiv.cal">

    <permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="com.example.mapdemo.permission.MAPS_RECEIVE" />

    <uses-feature
        android:glEsVersion="0x00020000"
        android:required="true"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version"/>
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyCUa9U7ZLDZuB_8hzz9Al-j27Vy5R-d6iA"/>

        <activity android:name=".Noterr.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Noterr.Calendar_view" />
        <activity android:name=".Noterr.Dashboard"/>

        <activity
            android:name=".Noterr.Day_view"
            android:label="@string/title_activity_day_view"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Noterr.Week_view"
            android:label="@string/title_activity_week_view"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Noterr.Month_view"
            android:label="@string/title_activity_month_view"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Noterr.Create_Event"></activity>
    </application>

</manifest>