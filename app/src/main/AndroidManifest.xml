<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.shiv.cal">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Noterr.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Noterr.Calendar_view" />
        <activity android:name=".Noterr.Dashboard"/>

        <activity
            android:name=".Noterr.Day_view"
            android:label="@string/title_activity_day_view"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Noterr.Week_view"
            android:label="@string/title_activity_week_view"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Noterr.Month_view"
            android:label="@string/title_activity_month_view"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Noterr.Create_Event"></activity>
    </application>

</manifest>